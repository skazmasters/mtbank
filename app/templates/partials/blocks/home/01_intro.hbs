<section class="home-main">
  <div class="home-main__container swiper-container js-hero-slider" data-autoplay-delay="10">
    <div class="home-main__list swiper-wrapper">
      <div class="swiper-slide">
        {{> hero/home-1}}
      </div>
      <div class="swiper-slide">
        {{> hero/home-2}}
      </div>
      <div class="swiper-slide">
        {{> hero/home-3}}
      </div>
      <div class="swiper-slide">
        {{> hero/home-4}}
      </div>
      <div class="swiper-slide">
        {{> hero/home-5}}
      </div>
    </div>

    <div class="home-main__pagination">
      <div class="swiper-pagination"></div>
    </div>

    <div class="home-main__nav">
      <div class="page__container">
        <div class="home-main__nav-inner">
          {{> components/slider-nav small=false fill=true indicator=true}}
        </div>
      </div>
    </div>
  </div>
</section>




